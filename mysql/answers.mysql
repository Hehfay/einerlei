USE Quiz;

DROP TABLE if EXISTS answers;

CREATE TABLE input(
  i TINYINT UNSIGNED NOT NULL,
  PRIMARY KEY(i)
);
INSERT INTO input VALUES(1, 2, 3, 4, 5);

